{
    "name": "canihasreview",
    "version": "0.0.2",
    "description": "GitHub Pull Request versioning utility",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=7.9.0"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "all": "npm-run-all",
        "build:webpack": "webpack --progress",
        "build": "npm-run-all build:webpack",
        "lint:tslint": "tslint -p tsconfig.json --type-check",
        "lint": "npm-run-all lint:tslint",
        "test": "mocha-webpack",
        "test-debug": "node --inspect --debug-brk node_modules/.bin/mocha-webpack",
        "start": "node dist/server",
        "watch:webpack": "webpack -w --progress",
        "watch:server": "nodemon --watch dist dist/server",
        "watch": "npm-run-all --parallel \"watch:*\"",
        "clean:dist": "rimraf dist",
        "clean": "npm-run-all \"clean:*\"",
        "prepublishOnly": "npm-run-all clean build"
    },
    "dependencies": {
        "source-map-support": "^0.4.15"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.41",
        "@types/node": "^7.0.18",
        "@types/sinon": "^2.2.1",
        "compression-webpack-plugin": "^0.4.0",
        "file-loader": "^0.11.1",
        "glob": "^7.1.1",
        "mocha": "^3.3.0",
        "mocha-typescript": "^1.1.2",
        "mocha-webpack": "^1.0.0-beta.1",
        "nodemon": "^1.11.0",
        "npm-run-all": "^4.0.2",
        "rimraf": "^2.6.1",
        "sinon": "^2.2.0",
        "ts-loader": "^2.0.3",
        "tslint": "^5.2.0",
        "tslint-language-service": "^0.9.6",
        "typescript": "^2.3.2",
        "webpack": "^2.5.1",
        "webpack-merge": "^4.1.0",
        "webpack-node-externals": "^1.6.0",
        "webpack-stats-plugin": "^0.1.5"
    }
}
