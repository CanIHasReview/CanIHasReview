{
    "name": "canihasreview",
    "version": "0.0.2",
    "description": "GitHub Pull Request versioning utility",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=8.9.0"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "all": "npm-run-all",
        "build:webpack": "webpack --progress",
        "build": "npm-run-all build:webpack",
        "lint:tslint": "tslint -p tsconfig.json",
        "lint": "npm-run-all lint:tslint",
        "test": "mocha-webpack",
        "test-debug": "node --inspect-brk node_modules/.bin/mocha-webpack",
        "cover": "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text npm run test",
        "start": "node dist/server",
        "watch:webpack": "webpack -w --progress",
        "watch:server": "nodemon --watch dist dist/server",
        "watch": "npm-run-all --parallel \"watch:*\"",
        "clean:dist": "rimraf dist",
        "clean": "npm-run-all \"clean:*\"",
        "prepublishOnly": "npm-run-all clean build"
    },
    "nyc": {
        "include": [
            "lib/**/*.ts",
            "webui/**/*.ts",
            "webui/**/*.tsx"
        ],
        "instrument": false,
        "sourceMap": false
    },
    "dependencies": {
        "bootstrap": "^4.3.0",
        "dotenv": "^4.0.0",
        "fetch-ponyfill": "^4.0.0",
        "fs-extra": "^5.0.0",
        "http-errors": "^1.7.1",
        "jquery": "^3.2.1",
        "jsonwebtoken": "^8.4.0",
        "lodash": "^4.17.11",
        "mime": "^2.4.0",
        "nock": "^9.6.1",
        "path-to-regexp": "^2.4.0",
        "popper.js": "^1.14.7",
        "querystringify": "^1.0.0",
        "react": "^15.5.4",
        "react-dom": "^15.5.4",
        "react-transition-group": "^1.1.3",
        "reactstrap": "^4.5.0",
        "simple-oauth2": "^1.6.0",
        "source-map-support": "^0.4.18",
        "tmp": "0.0.31",
        "url-parse": "^1.4.4",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@types/dotenv": "^4.0.3",
        "@types/fs-extra": "^5.0.5",
        "@types/http-errors": "^1.6.1",
        "@types/jsonwebtoken": "^7.2.8",
        "@types/lodash": "^4.14.120",
        "@types/mime": "^2.0.1",
        "@types/mocha": "^2.2.48",
        "@types/nock": "^9.3.1",
        "@types/node": "^7.10.3",
        "@types/react": "^15.6.21",
        "@types/react-dom": "^15.5.8",
        "@types/react-transition-group": "^1.1.5",
        "@types/reactstrap": "^4.6.8",
        "@types/simple-oauth2": "^1.6.0",
        "@types/sinon": "^2.2.1",
        "@types/tmp": "0.0.33",
        "@types/url-parse": "^1.4.3",
        "@types/uuid": "^3.4.4",
        "compression-webpack-plugin": "^0.4.0",
        "cross-env": "^5.2.0",
        "file-loader": "^0.11.1",
        "glob": "^7.1.3",
        "istanbul-instrumenter-loader": "^3.0.1",
        "mocha": "^3.3.0",
        "mocha-typescript": "^1.1.17",
        "mocha-webpack": "^1.1.0",
        "nodemon": "^1.18.10",
        "npm-run-all": "^4.1.5",
        "nyc": "^11.9.0",
        "rimraf": "^2.6.3",
        "sinon": "^2.2.0",
        "spinkit": "^1.2.5",
        "ts-loader": "^2.0.3",
        "tslint": "^5.12.1",
        "tslint-language-service": "^0.9.9",
        "typescript": "^2.9.2",
        "webpack": "^2.5.1",
        "webpack-merge": "^4.2.1",
        "webpack-node-externals": "^1.7.2",
        "webpack-stats-plugin": "^0.1.5"
    }
}
